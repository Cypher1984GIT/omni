{
  "name": "omni",
  "version": "1.4.3",
  "description": "Omni - A multi-AI browser interface",
  "repository": "github:Cypher1984GIT/Omni",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Cypher1984 <https://github.com/Cypher1984GIT>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.5.0"
  },
  "overrides": {
    "tar": "^7.5.6",
    "@electron/rebuild": {
      "tar": "^6.2.1"
    }
  },
  "build": {
    "appId": "com.omni.app",
    "productName": "Omni",
    "icon": "build/icon.png",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility;Network;WebBrowser;",
      "executableName": "omni",
      "maintainer": "Cypher1984 <https://github.com/Cypher1984GIT>",
      "icon": "build/icons"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ],
      "category": "public.app-category.utilities",
      "icon": "build/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "files": [
      "main.js",
      "index.html",
      "error.html",
      "icon.png",
      "icon.ico",
      "package.json",
      "preload.js"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Cypher1984GIT",
        "repo": "Omni",
        "releaseType": "release"
      }
    ]
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2"
  }
}